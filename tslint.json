{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-self-close": false,
    "interface-name": false,
    "no-empty": false,
    "ordered-imports": false,
    "no-empty-interface": false,
    "variable-name": false,
    "no-var-requires": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/slidarClassic/**"
    ]
  }
}
